{
  "name": "heroku",
  "displayName": "Heroku",
  "description": "Heroku VSCode extension",
  "repository": "https://github.com/heroku/heroku-vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": ">=1.91.0",
    "node": ">=22.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:shellscript",
    "onAuthenticationRequest:<Heroku>"
  ],
  "extensionDependencies": [
    "vscode.git"
  ],
  "main": "./out/extension/index.js",
  "contributes": {
    "viewsWelcome": [
      {
        "view": "heroku:auth:login",
        "contents": "%heroku:auth:login:contents%"
      },
      {
        "view": "heroku:getting-started",
        "contents": "%heroku:getting-started:contents%"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "heroku",
          "title": "%view.heroku.name%",
          "icon": "/resources/heroku.png"
        }
      ]
    },
    "views": {
      "heroku": [
        {
          "id": "heroku:auth:login",
          "name": "%view.heroku.name%",
          "when": "!heroku.authenticated"
        },
        {
          "id": "heroku:resource-explorer:treeview",
          "name": "%view.heroku.resourceExplorer%",
          "when": "heroku.authenticated && heroku.app-found"
        },
        {
          "id": "heroku:getting-started",
          "name": "%view.heroku.name%",
          "when": "heroku.authenticated && !heroku.app-found"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "when": "viewItem === dyno:up || viewItem === dyno:down || viewItem === dyno:crashed",
          "command": "heroku:dyno:restart",
          "group": "inline"
        },
        {
          "when": "viewItem === formation:scale-up || viewItem === formation:scale-down || viewItem === formation:scale-up:scale-down",
          "command": "heroku:scale-formation",
          "group": "inline"
        },
        {
          "when": "viewItem === formation:scale-up || viewItem === formation:scale-down || viewItem === formation:scale-up:scale-down",
          "command": "heroku:scale-formation"
        },
        {
          "when": "viewItem === heroku:app || viewItem === heroku:app:log-session-started",
          "command": "heroku:open-app",
          "group": "inline@1"
        },
        {
          "when": "viewItem === heroku:app || viewItem === heroku:app:log-session-started",
          "command": "heroku:access-settings",
          "group": "inline@2"
        },
        {
          "when": "viewItem === heroku:app",
          "command": "heroku:start-log-session",
          "group": "inline@3"
        },
        {
          "when": "viewItem === heroku:app:log-session-started",
          "command": "heroku:end-log-session",
          "group": "inline@3"
        },
        {
          "when": "viewItem === heroku:app || viewItem === heroku:app:log-session-started",
          "command": "heroku:open-app",
          "group": "App@1"
        },
        {
          "when": "viewItem === heroku:app || viewItem === heroku:app:log-session-started",
          "command": "heroku:access-settings",
          "group": "App@2"
        },
        {
          "when": "viewItem === heroku:app",
          "command": "heroku:start-log-session",
          "group": "App@3"
        },
        {
          "when": "viewItem === heroku:app:log-session-started",
          "command": "heroku:end-log-session",
          "group": "App@3"
        }
      ]
    },
    "commands": [
      {
        "command": "heroku:addons:show-addons-view",
        "title": "Find more add-ons"
      },
      {
        "command": "heroku:scale-formation",
        "title": "Scale",
        "icon": "$(pencil)"
      },
      {
        "command": "heroku:dyno:restart",
        "title": "Restart",
        "icon": "$(timeline-refresh)"
      },
      {
        "command": "heroku:open-app",
        "title": "Open app in browser",
        "icon": "$(link-external)"
      },
      {
        "command": "heroku:start-log-session",
        "title": "Start log session",
        "icon": "$(list-selection)"
      },
      {
        "command": "heroku:end-log-session",
        "title": "End log session",
        "icon": "$(debug-disconnect)"
      },
      {
        "command": "heroku:access-settings",
        "title": "Access app settings",
        "icon": "$(gear)"
      }
    ],
    "capabilities": {
      "hoverProvider": true
    },
    "languages": [
      {
        "id": "heroku-logs",
        "aliases": [
          "Heroku Logs",
          "heroku-logs"
        ],
        "extensions": [
          ".log",
          ".heroku.log"
        ],
        "configuration": "./configurations/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "heroku-logs",
        "scopeName": "source.heroku-logs",
        "path": "./grammars/heroku-log-format.json"
      }
    ],
